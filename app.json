{
  "expo": {
    "name": "Bet.AI",
    "slug": "betai",
    "version": "1.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "betai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.betai.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Bet.AI needs access to your camera to scan physical bet slips, allowing you to quickly import bet details without manual entry. For example, you can point your camera at a printed ticket to automatically add the bet for AI analysis.",
        "NSPhotoLibraryUsageDescription": "Bet.AI needs access to your photo library so you can select images of bet slips (like screenshots) for analysis. For example, you can choose a saved image of an online bet slip to import its details into the app."
      },
      "appleTeamId": "GQ534WU43T"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/images/android-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.awesome.betai",
      "versionCode": 8,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "betai"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#0C0C0C"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts"]
        }
      ],
      "expo-secure-store",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "312dd5ab-fbb2-47a5-87e3-879b0f148b8f"
      }
    },
    "owner": "0xcez-tac"
  }
}
